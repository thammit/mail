    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "plugin"
    ],
    "homepage": "https://www.typo3-webagentur.com",
    "license": "GPL-2.0-or-later",
    "support": {
        "issues": "https://gitlab.typo3-webagentur.com/mediaessenz/mail/issues"
    },
    "require": {
        "typo3/cms-core": "^11.5",
        "friendsoftypo3/tt-address": "^6",
        "friendsoftypo3/rdct": "^2.1",
        "friendsoftypo3/jumpurl": "^8",
        "tedivm/fetch": "^0.7",
        "league/html-to-markdown": "^5",
        "pelago/emogrifier": "^6"
